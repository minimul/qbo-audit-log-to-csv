{
  "name": "QboAuditLog to CSV",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "QboAuditLog to CSV",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "https://*.intuit.com/*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.intuit.com/*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
